<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.media.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="613.0" prefWidth="1007.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginController">
   <children>
      <ImageView fx:id="LoginPic" fitHeight="600.0" fitWidth="2500.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../photos/Login.jpg" />
         </image>
      </ImageView>
      <Pane layoutX="-7.0">
         <children>
            <MediaView fx:id="video" fitHeight="700.0" fitWidth="1000.0"  />
            <Button fx:id="btnSkip" layoutX="845.0" layoutY="490.0" mnemonicParsing="false" onAction="#clickSkip" stylesheets="@Css/TableCss.css" text="Skip" />
            <Button fx:id="btnLogin" layoutX="445.0" layoutY="365.0" mnemonicParsing="false" onAction="#ClickLogin" prefHeight="28.0" prefWidth="88.0" stylesheets="@Css/TableCss.css" text="Login" />
            <TextField fx:id="txtnameLogin" layoutX="418.0" layoutY="332.0" promptText="User name" />
         </children>
      </Pane>
   </children>
</AnchorPane>
